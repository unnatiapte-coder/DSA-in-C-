{
case 1:

printf(&quot;Enter data=&quot;);
scanf(&quot;%d&quot;,&amp;num);
add(num);
display();
break;
case 2:
num=delete();
if(num!=-9999)
{
printf(&quot;Element popped=%d&quot;,num);
display();
}
break;
}
}while(ch&lt;3);
}
void init()
{
front=NULL;
rear=NULL;
}
