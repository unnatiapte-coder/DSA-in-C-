{
p=(struct stack*)malloc(sizeof(struct stack));
p-&gt;number=num;
p-&gt;next=NULL;
if (top==NULL)
{
}
else
{

}
}

top=p;

p-&gt;next=top;
top=p;

int empty()
{
if(top==NULL)
{
}
else
{
}
}

return 1;

return 0;

void display()
{
int i;
if(top==NULL)
{
}
else
{
printf(&quot;Stack is empty&quot;);

p=top;
while(p!=NULL)
{
printf(&quot;\n\t%d&quot;,p-&gt;number);
p=p-&gt;next;
}
}
}
int pop()
{
int num;
if(empty()==1)
{

}
else
{
printf(&quot;\n Stack Empty&quot;);
return -9999;

p=top;
num=top-&gt;number;
top=top-&gt;next;
