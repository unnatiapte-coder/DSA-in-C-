#include&lt;stdio.h&gt;
#include&lt;stdlib.h&gt;
#define MAX 5
struct stack
{
int number;
struct stack *next;
}*top,*p;
void init();
void push (int);
int full();
int pop();
int empty();
void display();
int peak();
int main()
{
int ch,num;
init();
do
{
printf(&quot;\n Menu:\n 1.Push \n 2.Pop&quot;);
scanf(&quot;%d&quot;,&amp;ch);
switch(ch)
{
case 1:
printf(&quot;Enter data=&quot;);
scanf(&quot;%d&quot;,&amp;num);
push(num);
display();
break;
case 2:
num=pop();

if(num!=-9999)
{
printf(&quot;Element popped=%d&quot;,num);
display();
}
break;
}
}while(ch&lt;3);
}
void init()
{
top=NULL;
}
